<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="117.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.DeleteDialogController">
   <children>
      <Text fx:id="text" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure want to delete this" textAlignment="CENTER" wrappingWidth="442.0">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <ButtonBar layoutX="99.0" layoutY="59.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="OkBtn" onMouseClicked="#handleOK" mnemonicParsing="false" text="Yes" />
            <Button fx:id="cancelBtn" onMouseClicked="#handleCancel" mnemonicParsing="false" text="No" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
